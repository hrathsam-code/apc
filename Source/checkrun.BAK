                     %IFNDEF $CheckRunVAR
$CheckRunVAR                     EQUATE         1
;
;
;         Field Definitions                              
;================================================================================================;
;                                          
;                                          
. Field                           Type            Length             From - To       Description
;--------------------------------------------------------------------------------------------------------------
CheckRun                         RECORD
Entity                           Dim                8                   1 - 8        //1 - 8
SubEntity                        Dim                8                   9 - 16       //9 - 16
Vendor                           Dim                6                  17 - 22       //17 - 22
BankCode                         Dim                6                  23 - 28       //23 - 28
CheckNo                          Form               10                 29 - 38       //29 - 38
CheckDate                        Dim                8                  39 - 46       //39 - 46
Year                             Dim                4                  47 - 50       //47 - 50
Month                            Dim                2                  51 - 52       //51 - 52
CheckAmount                      Form               10.2               53 - 65       //53 - 65       //Check Amount = GrossAmou
DiscAmt                          Form               7.2                66 - 75       //66 - 75
GrossAmount                      Form               10.2               76 - 88       //76 - 88       //HR 6/1/2005
CheckFlag                        Dim                1                  89 - 89       //89 - 89       //M = Manual
Printed                          Form               1                  90 - 90       //90 - 90       //0=Not Printed, 1=Printed
SeqMajor                         Form               7                  91 - 97       //91 - 97
TransDate                        Dim                8                  98 - 105      //107 - 114
Memo                             Dim                60                106 - 165      //115 - 174
                                 RECORDEND
;
;======================================================================================;
TempCheckRun                        record          like   CheckRun                        
OrigCheckRun                        record          like   CheckRun                        
SaveCheckRun                        record          like   CheckRun                        
;======================================================================================;
; 
; File Keys and File Labels  
; --------------------------------------------------  
CheckRunTXTNM                          INIT         "CheckRun"
CheckRunISI1NM                         INIT               "CheckRun.ISI"
CheckRunI1DEF                           init            "-n,z,y,1-8,17-22,23-28,91-97"
;
; By Entity, Vendor, BankCode, SeqMajor
; 1-8, 17-22, 23-28, 91-97
;
CheckRunKY                       DIM                27
;=============================================================================;
;
CheckRunFLST                         FILELIST
CheckRunFL                          IFILE              name="CheckRun.ISI",FIX=165,NODUP,NOPATH
                                    FILELISTEND
 
CheckRunFileSize       CONST      "165"
 
CheckRunProg           INIT       "CheckRun Program"
CheckRunDesc           INIT       "CheckRun Maintenance"
CheckRunTITLE          INIT       "CheckRun Record"
CheckRunHelp           INIT       "CheckRun.HLP"
 
 
 
 
          %ENDIF          //End of FD Include File
 
 
          %IFNDEF $CheckRunVARIO
            INCLUDE CheckRun.IO 
