           %ENDIF
           %IFDEF            APTypeKY6
APTypeFL6   IFILE             Name=APTypeISI6NM,NODUP,NOPATH,UPPER
           %ENDIF
           %IFDEF            APTypeKY7
APTypeFL7   IFILE             Name=APTypeISI7NM,NODUP,NOPATH,UPPER
           %ENDIF
           %IFDEF            APTypeKY8
